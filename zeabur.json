{
  "name": "yougame-explorer",
  "type": "python",
  "build": {
    "dockerfile": "Dockerfile.simple"
  },
  "env": {
    "OPENAGENTS_HOST": "0.0.0.0",
    "OPENAGENTS_PORT": "8000",
    "PYTHONUNBUFFERED": "1",
    "PYTHONDONTWRITEBYTECODE": "1"
  },
  "port": 8000,
  "healthcheck": {
    "path": "/health",
    "timeout": 30,
    "interval": 60
  }
}