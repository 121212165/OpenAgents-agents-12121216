{
  "name": "yougame-explorer",
  "type": "prebuilt-dockerfile",
  "dockerfile": "Dockerfile",
  "env": {
    "OPENAGENTS_HOST": "0.0.0.0",
    "PYTHONUNBUFFERED": "1",
    "PYTHONDONTWRITEBYTECODE": "1"
  },
  "ports": [
    {
      "port": 8000,
      "protocol": "TCP"
    }
  ],
  "healthcheck": {
    "path": "/health",
    "timeout": 60,
    "interval": 60,
    "retries": 5
  }
}
